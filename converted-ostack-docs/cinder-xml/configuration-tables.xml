  <table>
    <title>Deprecated options</title>
    <tgroup cols="2">
      <colspec colname="c1" colwidth="50.0*"/>
      <colspec colname="c2" colwidth="50.0*"/>
      <thead>
        <row>
          <entry>
            <para>Deprecated option</para>
          </entry>
          <entry>
            <para>New Option</para>
          </entry>
